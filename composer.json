{
    "name": "dwolke/zf-mailer",
    "description": "Simple Mailer for Zend Framework 3",
    "version": "3.0.0",
    "type": "library",
    "keywords": ["e-mail", "mailer", "php", "zend framework", "zf2"],
    "homepage": "http://zfmailer.dw-labs.de",
    "license": "MIT",
    "authors":[
        {
            "name": "Daniel Wolkenhauer",
            "email": "hello@dw-labs.de",
            "homepage": "http://dw-labs.de",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-stdlib": "^3.2",
        "zendframework/zend-servicemanager": "^3.4",
        "zendframework/zend-mail": "^2.10",
        "zendframework/zend-view": "^2.11"
    },
    "autoload": {
        "psr-0": {
            "ZfMailer": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpmd/phpmd": "^2.7",
        "squizlabs/php_codesniffer": "^3.4"
    }
}
