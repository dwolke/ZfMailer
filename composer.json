{
    "name": "dwolke/zf-mailer",
    "description": "Simple Mailer for Zend Framework",
    "version": "1.0.0",
    "type": "library",
    "keywords": ["e-mail", "mailer", "php", "zend framework", "zf2"],
    "homepage": "http://zfmailer.dw-labs.de",
    "license": "MIT",
    "authors":[
        {
            "name": "Daniel Wolkenhauer",
            "email": "hello@dw-labs.de",
            "homepage": "http://dw-labs.de",
            "role": "developer"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "zendframework/zend-stdlib": "~2.4",
        "zendframework/zend-servicemanager": "~2.4",
        "zendframework/zend-mail": "~2.4",
        "zendframework/zend-view": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "phpmd/phpmd": "^2.4",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-0": {
            "ZfMailer": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
